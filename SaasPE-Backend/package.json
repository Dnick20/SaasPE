    "@aws-sdk/client-sesv2": "^3.922.0",
    "@aws-sdk/client-sqs": "^3.926.0",
    "@aws-sdk/s3-request-presigner": "^3.913.0",
    "@nestjs/bull": "^11.0.4",
    "@nestjs/common": "^11.0.1",
    "@nestjs/config": "^4.0.2",
    "@nestjs/core": "^11.0.1",
    "@nestjs/jwt": "^11.0.1",
    "@nestjs/passport": "^11.0.5",
    "@nestjs/platform-express": "^11.1.7",
    "@nestjs/platform-socket.io": "^11.1.7",
    "@nestjs/schedule": "^6.0.1",
    "@nestjs/swagger": "^11.2.1",
    "@nestjs/throttler": "^6.4.0",
    "@nestjs/websockets": "^11.1.7",
    "@opentelemetry/api": "1.9.0",
    "@opentelemetry/core": "1.30.1",
    "@opentelemetry/sdk-trace-base": "1.30.1",
    "@opentelemetry/semantic-conventions": "1.37.0",
    "@prisma/client": "^6.17.1",
    "@sentry/node": "^10.21.0",
    "@sentry/profiling-node": "^10.22.0",
    "@types/cookie-parser": "^1.4.10",
    "@types/diff-match-patch": "^1.0.36",
    "@types/passport-apple": "^2.0.3",
    "@types/passport-google-oauth20": "^2.0.16",
    "@types/passport-microsoft": "^2.1.0",
    "axios-retry": "^4.5.0",
    "bcrypt": "^6.0.0",
    "bull": "^4.16.5",
    "cheerio": "^1.1.2",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.2",
    "cookie-parser": "^1.4.7",
    "dd-trace": "^5.73.0",

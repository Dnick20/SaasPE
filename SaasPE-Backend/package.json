{
  "name": "SaasPE-Backend",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "test:auth": "jest --config ./test/jest-e2e.json --testPathPattern=auth",
    "test:auth:integration": "jest src/modules/auth/__tests__/auth.controller.spec.ts",
    "test:auth:e2e": "jest --config ./test/jest-e2e.json test/e2e/auth.e2e-spec.ts",
    "test:auth:all": "npm run test:auth:integration && npm run test:auth:e2e",
    "test:setup": "node -e \"console.log('Setting up test database...')\" && DATABASE_URL=postgresql://dev:dev@localhost:5432/saaspe_test npx prisma migrate deploy && DATABASE_URL=postgresql://dev:dev@localhost:5432/saaspe_test npm run prisma:seed",
    "prisma:generate": "prisma generate",
    "prisma:migrate": "prisma migrate dev",
    "prisma:seed": "ts-node prisma/seed.ts",
    "seed:production": "NODE_ENV=production ts-node scripts/seed-production.ts",
    "prisma:studio": "prisma studio"
  },
  "prisma": {
    "seed": "ts-node prisma/seed.ts"
  },
